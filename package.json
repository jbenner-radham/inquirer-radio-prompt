{
  "name": "inquirer-radio-prompt",
  "version": "1.1.0",
  "description": "An Inquirer radio button prompt.",
  "keywords": [
    "answer",
    "cli",
    "command-line",
    "input",
    "inquirer",
    "prompt",
    "radio",
    "radio-button",
    "stdin",
    "stdout",
    "terminal",
    "tty",
    "tui",
    "ui"
  ],
  "repository": "github:jbenner-radham/inquirer-radio-prompt",
  "license": "MIT",
  "author": "James Benner <james.benner@gmail.com> (https://www.jamesbenner.com/)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "test": "vitest --watch=false",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@inquirer/core": "^10.0.0",
    "@inquirer/figures": "^1.0.7",
    "@inquirer/type": "^3.0.0",
    "ansi-escapes": "^7.0.0",
    "yoctocolors": "^2.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.0",
    "@eslint/js": "^9.12.0",
    "@inquirer/testing": "^2.1.35",
    "eslint": "^9.12.0",
    "globals": "^15.10.0",
    "husky": "^9.1.6",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.1",
    "vitest": "^2.1.2"
  },
  "engines": {
    "node": ">=18"
  }
}
